#include<stdio.h>
#include<iostream>
#include<algorithm>

void sorting(int a[],int array_size){

for(int i=0; i < array_size; i++){

cout<<a[i]<<"";
}

}

int main(){

int a[]={5,1,8,6,9,22,11,10};

int asize = sizeof(a) / sizeof(a[0]);
cout<<"The Array Before Sort:\n";

sorting(a,asize);

sort(a,a + asize);

cout<<"\nThe Array After sorting is:\n";

sorting

}

















